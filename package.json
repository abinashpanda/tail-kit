{
  "name": "@tail-kit/tail-kit",
  "description": "UI-Kit made with tailwindcss",
  "version": "0.1.13",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "license": "MIT",
  "keywords": [
    "react",
    "ui-kit",
    "tailwindcss"
  ],
  "workspaces": [
    "packages/*",
    "website"
  ],
  "private": true,
  "scripts": {
    "lint": "eslint packages/**/*.ts packages/**/*.tsx packages/**/*.js --fix",
    "format": "prettier --write --no-error-on-unmatched-pattern packages/**/*.ts packages/**/*.tsx packages/**/*.js"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "dayjs": "^1.9.3"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "autoprefixer": "^9.8.6",
    "commitizen": "^4.2.2",
    "concurrently": "^5.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lerna": "^4.0.0",
    "postcss": "^8.2.4",
    "prettier": "^2.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.2.2",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dopplr-labs/tail-kit.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
